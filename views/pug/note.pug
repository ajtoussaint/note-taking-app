include /universalHeader

doctype html
head
  link(rel='stylesheet' type='text/css' href='../public/styles.css')
body
+universalHeader
#noteContent.content
  h1
    |#{title}
  h3
    |Created on: #{createdOn}
  #noteMainContent
    !=content
  #noteTags
    each tag in tags
      .noteTag
        |#{tag}
